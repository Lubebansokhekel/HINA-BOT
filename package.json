{
  "name": "galirus-bot",
  "version": "3.0.0",
  "description": "WhatsApp HINA-BOT GALIRUS dengan Web Pairing",
  "main": "main.js",
  "type": "module",
 "scripts": {
  "start": "bash hapus_session_lama.sh",
  "dev": "while true; do node index.js; echo \"ðŸ”„ Restarting...\"; sleep 1; done",
  "hot-reload": "node main.js",
  "monitor": "node index.js",
  "pairing": "node index.js --pairing",
  "clean": "rm -rf sessions files/downloads",
  "setup": "pkg update && pkg install python ffmpeg yarn tgpt -y && pip install yt-dlp",
  "install-all": "yarn && npm run setup",
  "check-deps": "node -e \"import('child_process').then(({exec}) => exec('which yt-dlp', (e,s) => console.log(e?'yt-dlp not installed':'yt-dlp ready')))\"",
  "fix-permission": "chmod -R 755 sessions files",
  "test": "echo \"No tests specified\" && exit 0"
},
